<?xml version="1.0" encoding="utf-8"?>
<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <AssemblyName>FsCheck.Xunit</AssemblyName>
    <TargetFramework>netstandard2.0</TargetFramework>
    <GenerateDocumentationFile>true</GenerateDocumentationFile>
    <GenerateAssemblyInfo>false</GenerateAssemblyInfo>
    <Product>FsCheck.XunitDrive2</Product>
    <Authors>Kurt Schelfthout and contributors and Drive2</Authors>
    <Copyright>Copyright 2024</Copyright>
    <PackageProjectUrl>https://github.com/renatmedved/FsCheck</PackageProjectUrl>
    <RepositoryUrl>https://github.com/renatmedved/FsCheck</RepositoryUrl>
    <PackageTags>F# fsharp test random drive2</PackageTags>
    <Description>FsCheck.Xunit Drive2 edition</Description>
  </PropertyGroup>
  <ItemGroup>
    <Compile Include="AssemblyInfo.fs" />
    <Compile Include="Runner.fs" />
    <Compile Include="PropertyAttribute.fs" />
    <Compile Include="CheckExtensions.fs" />
  </ItemGroup>
  <ItemGroup>
    <None Include="paket.references" />
    <None Include="paket.template" />
  </ItemGroup>
  <ItemGroup>
    <ProjectReference Include="../FsCheck/FsCheck.fsproj" />
  </ItemGroup>
  <Import Project="..\..\.paket\Paket.Restore.targets" />
</Project>